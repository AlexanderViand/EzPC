include(CMakeFindDependencyMacro)

@PACKAGE_INIT@

set(CMAKE_MODULE_PATH ${CMAKE_CURRENT_LIST_DIR})
include("${CMAKE_CURRENT_LIST_DIR}/source_of_randomness.cmake")

find_dependency(OpenSSL REQUIRED)
